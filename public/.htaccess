# This file should be in your PUBLIC folder
RewriteEngine On
RewriteBase /

# Force /checkout to serve checkout.html internally
RewriteRule ^checkout$ checkout.html [L]

# Redirect /checkout.html to /checkout for clean URLs
RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s([^.]+)\.html [NC]
RewriteRule ^ %1 [R=301,L]